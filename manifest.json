{
    "manifest_version": 3,
    "name": "TabsGroupSaver",
    "version": "0.1",
    "action": {
        "default_icon": "assets/table-refresh.png",
        "default_title": "TabsGroupSaver",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "storage"
    ],
    "background": {
        "service_worker": "background.js",
        "type": "module"
    }
}